{
	"name": "@nan0web/llimo.app",
	"version": "1.0.0",
	"description": "",
	"main": "bin/llimo-chat.js",
	"type": "module",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./release": {
			"types": "./types/release/index.d.ts",
			"import": "./src/release/index.js"
		},
		"./utils": {
			"types": "./types/utils/index.d.ts",
			"import": "./src/utils/index.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"clean:modules": "rm -rf node_modules/",
		"play": "node play/main.js",
		"test": "node --test --test-timeout=3333 'src/**/*.test.js'",
		"test:all": "npm run test && npm run build",
		"test:chat": "node bin/llimo.js chat --test=chat-dir",
		"test:docs": "node --test --test-timeout=3333 src/README.md.js",
		"test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
		"test:reproduce": "node bin/llimo.js chat --test-dir=/path/to/chat/archive --input test-prompt.md",
		"test:releases": "node --test --test-timeout=3333 'releases/**/task.test.js' && node --test --test-timeout=3333 'releases/*/*/index.test.js'",
		"test:release:v1.0.0": "node --test --test-timeout=3333 'releases/1/v1.0.0/*/task.test.js'",
		"knip": "knip",
		"knip:production": "knip --production",
		"prepare": "husky",
		"initKey": "dotenv node bin/llimo chat --fast"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@nan0web/test": "^1.1.3",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"knip": "^5.42.2",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@ai-sdk/cerebras": "^1.0.32",
		"@ai-sdk/huggingface": "^1.0.2",
		"@ai-sdk/openai": "^2.0.86",
		"@openrouter/ai-sdk-provider": "^1.4.0",
		"ai": "^5.0.107",
		"micromatch": "^4.0.8",
		"yaml": "^2.8.2"
	},
	"bin": {
		"llimo": "bin/llimo.js",
		"llimo-release": "bin/llimo-release.js",
		"llimo-stats": "bin/stats.js"
	}
}